<?xml version="1.0"?>
<xs:schema version="1.0"
           xmlns:xs="http://www.w3.org/2001/XMLSchema"
           targetNamespace="www.kimthanh.com/schema/list-product"
           xmlns="www.kimthanh.com/schema/list-product"
           xmlns:product="www.kimthanh.com/schema/product"
           elementFormDefault="qualified">
    <xs:import namespace="www.kimthanh.com/schema/product" schemaLocation="product.xsd"/>

    <xs:element name="ListProduct">
        <xs:complexType>
            <xs:sequence>
                <xs:element ref="product:Product" maxOccurs="unbounded"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>
</xs:schema>
